[32m+[m[32m    private static int total = 0;[m
[32m+[m[41m    [m
[32m+[m[32m    public Conta(int agencia, int numero){[m
[32m+[m[32m        Conta.total++;[m
[32m+[m[32m        //System.out.println("O total de contas √© " + Conta.total);[m
[32m+[m[32m        this.agencia = agencia;[m
[32m+[m[32m        this.numero = numero;[m
[32m+[m[32m        //this.saldo = 100;[m
[32m+[m[32m        //System.out.println("Estou criando uma conta " + this.numero);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public abstract void deposita(double valor);[m
[32m+[m
[32m+[m[32m    public boolean saca(double valor) {[m
[32m+[m[32m        if(this.saldo >= valor) {[m
[32m+[m[32m            this.saldo -= valor;[m
[32m+[m[32m            return true;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public boolean transfere(double valor, Conta destino) {[m
[32m+[m[32m        if(this.saca(valor)) {[m
[32m+[m[41m        [m		[32mdestino.deposita(valor);[m
[32m+[m[41m        [m		[32mreturn true;[m
[32m+[m[32m        } else {[m
[32m+[m[41m        [m		[32mreturn false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public double getSaldo(){[m
[32m+[m[32m        return this.saldo;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public int getNumero(){[m
[32m+[m[32m        return this.numero;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setNumero(int numero){[m
[32m+[m[32m        if(numero <= 0) {[m
[32m+[m[32m            System.out.println("Nao pode valor menor igual a 0");[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        this.numero = numero;[m
[32m+[m[32m    }[m
[32m+[m
